{
    "compilerOptions": {
      "target": "es6",
      "module": "es2015",
      "lib": [
        "dom",
        "es6",
        "dom.iterable",
        "scripthost"
      ],
      "sourceMap": true,
      "rootDir": "./src",
      "outDir": "./build",
      "removeComments": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
    }
  }
  